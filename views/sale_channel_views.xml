<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_sale_channel_tree" model="ir.ui.view">
        <field name="name">sale.channel.tree</field>
        <field name="model">sale.channel</field>
        <field name="arch" type="xml">
            <tree string="Sales Channels" multi_edit="1" editable="top">
                <field name="name"/>
                <field name="code"/>
                <field name="warehouse_id"/>
                <field name="journal_id"/>
            </tree>
        </field>
    </record>
    <record id="action_sale_channel" model="ir.actions.act_window">
        <field name="name">Sales Channels</field>
        <field name="res_model">sale.channel</field>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="view_sale_channel_tree"/>
    </record>
    <menuitem id="menu_sale_config_channel" name="Sale Channels"
          parent="sale.menu_sale_config"
          groups="sales_team.group_sale_manager"
          action="action_sale_channel" sequence="5"/>
</odoo>